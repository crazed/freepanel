$TTL 1h
<DOMAIN>.		1h	IN  SOA  ns1.freepanel.org. admin@freepanel.org. (
				2010012001	; serial number
				1d	; refresh
				1d	; retry
				4w	; expire
				1h	; minimum TTL
				)
;
; Zone NS Records
;

@			NS	ns1.freepanel.org.

;
; Zone MX Records
;

@			MX	10  mailer1.freepanel.org.
@			MX	20  mailer2.freepanel.org.

;
; Zone Records
;

@		IN	A	<IP_ADDR>
www		IN	A	<IP_ADDR>
ftp		IN	A	<IP_ADDR>
mail		IN	A	<IP_ADDR>
